{% comment %} ATTENTION!!! This is compiled version of clinical-proven-content.liquid and it is overriding automatically. You should use theme development tool and sources\snippets\clinical-proven-content.liquid file to code and save your changes. {% endcomment %}{% assign set = section.settings %}
<style>
  #shopify-section-{{ section.id }} {
    padding: {{ set.ptop }}px 0 {{ set.pbottom }}px;
    background-color: {{ set.sectionbg }};
  }
  @media (max-width: 989px) {
    #shopify-section-{{ section.id }} {
      padding: {{ set.ptop_mob }}px 0 {{ set.pbottom_mob }}px;
    }
  }
</style>


<div class="container">
  <div class="section__content">
    {% for block in section.blocks %}
      {% assign bset = block.settings %}
      {% if bset.visible == false %} {% continue %} {% endif %}
      {% assign title = '' %}

      {%- capture title -%}
        {%- if bset.pretitle != blank -%}
          <h3 class="pretitle not-animated">{{ bset.pretitle }}</h3>
        {%- endif -%}
        {%- if bset.title != blank -%}
          <h2 class="title m-black not-animated">{{ bset.title }}</h2>
        {%- endif -%}
        {%- if bset.subtitle != blank -%}
          <div class="subtitle not-animated">{{ bset.subtitle }}</div>
        {%- endif -%}
      {%- endcapture -%}


      {%- case block.type -%}
        {%- when "title-top" -%}
          <div class="clinical m-top">
            {{ title }}
            <div class="clinical__grid">
              <div class="clinical__cell">
                <div class="clinical__emphasis">{{ bset.emphasis1 }}</div>
                <div class="clinical__text">{{ bset.text1 }}</div>
              </div>              
              <div class="clinical__cell">
                <div class="clinical__emphasis">{{ bset.emphasis2 }}</div>
                <div class="clinical__text">{{ bset.text2 }}</div>
              </div>              
              <div class="clinical__cell">
                <div class="clinical__emphasis">{{ bset.emphasis3 }}</div>
                <div class="clinical__text">{{ bset.text3 }}</div>
              </div>              
            </div>
          </div>
        {%- when "title-left" -%}
          <div class="clinical m-left">
            <div class="clinical__grid">
              {% if title != blank %}
                <div class="clinical__cell">
                  {{ title }}
                </div>
              {% endif %}
              <div class="clinical__cell">
                <div class="clinical__emphasis">{{ bset.emphasis1 }}</div>
                <div class="clinical__text">{{ bset.text1 }}</div>
              </div>              
              <div class="clinical__cell">
                <div class="clinical__emphasis">{{ bset.emphasis2 }}</div>
                <div class="clinical__text">{{ bset.text2 }}</div>
              </div>              
            </div>
          </div>
        {%- when "regular-text" -%}
          <div class="clinical m-regular">
            {{ title }}
          </div>
        {%- else -%}

      {%- endcase -%}
      
    {%- endfor -%}
  </div>
</div>
