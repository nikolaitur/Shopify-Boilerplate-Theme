{% comment %} ATTENTION!!! This is compiled version of password.liquid and it is overriding automatically. You should use theme development tool and sources\layout\password.liquid file to code and save your changes. {% endcomment %}<!doctype html>
<html class="no-js" lang="{{ shop.locale }}">
<head>
  {{ content_for_header }}
  {% include 'ea-head' %}
  <script src="{{ 'password.js' | asset_url }}" defer="defer"></script>
</head>

<body class="template-password">
  <div class="password-page__wrapper">
    {{ content_for_layout }}
  </div>

  <div id="LoginModal" class="modal modal--solid">
    <div class="modal__inner">
      <div class="modal__centered text-center">
        <div class="password-form">
          {% form 'storefront_password' %}
            {{ form.errors | default_errors }}
            <label for="password" class="hidden-label">Password</label>
            <div class="input-group">
              <input type="password" name="password" id="password" class="input-group-field" placeholder="Your password" autofocus>
              <span class="input-group-btn">
                <input type="submit" class="btn password-form__submit" value="Enter">
              </span>
            </div>
          {% endform %}
        </div>
      </div>
    </div>

    <button type="button" class="btn btn--tertiary btn--circle modal__close js-modal-close text-link">
      <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 64 64"><path d="M19 17.61l27.12 27.13m0-27.12L19 44.74"/></svg>
      <span class="icon__fallback-text">Close</span>
    </button>

    <div class="modal__footer-text password-admin-link">
      <small>
        Store owner? <a href="/admin" class="text-link">Log in here</a>
      </small>
    </div>
  </div>

  <script>
    var theme = {
      strings: {
        addressError: {{ 'sections.map.address_error' | t | json }},
        addressNoResults: {{ 'sections.map.address_no_results' | t | json }},
        addressQueryLimit: {{ 'sections.map.address_query_limit_html' | t | json }},
        authError: {{ 'sections.map.auth_error_html' | t | json }}
      }
    }
  </script>

</body>
</html>
